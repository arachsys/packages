prepare() {
  unpack https://www.kernel.org/pub/linux/kernel/v5.x/linux-5.9.14.tar.xz
  tree a90f8de2e78b75dbf2f04baee773f153c94b4ac3f6dd5a48d21a94026f6bfeda
}

build() {
  make headers
  find usr/include -type f -name '*.h' -exec cp -t $DST --parents {} +
  find usr/include -type l -exec cp -d -t $DST --parents {} +
  mv $DST/usr $DST/lib
}
