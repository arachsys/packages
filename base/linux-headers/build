prepare() {
  unpack https://www.kernel.org/pub/linux/kernel/v5.x/linux-5.8.12.tar.xz
  tree 13c36778e7571c12da5a410bf6421f37b0d6e20da7ddace837c5be921faafa5a
}

build() {
  make headers
  find usr/include -type f -name '*.h' -exec cp -t $DST --parents {} +
  find usr/include -type l -exec cp -d -t $DST --parents {} +
  mv $DST/usr $DST/lib
}
