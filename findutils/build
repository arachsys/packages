mode autotools

check() {
  url https://ftp.gnu.org/gnu/findutils/
  match version 4.8.0 'findutils-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.gnu.org/gnu/findutils/findutils-4.8.0.tar.xz
  apply paths.diff
  tree 3827368db69f3e09e6f1ab45d6ff2c70d302b1b8d3b5c752475eebc018d2e276
}

config-extra --libexecdir='${prefix}/bin' --localstatedir=/var/lib/misc
