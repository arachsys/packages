check() {
  url https://github.com/google/fscryptctl/tags
  match version 1.1.0 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack fscryptctl-1.1.0.tar.gz \
    https://github.com/google/fscryptctl/archive/refs/tags/v1.1.0.tar.gz
  apply cosmetic.diff
  tree 773bc1f2ebe2c70dafc2f5f0d7015e06ac31b4536dc046341d5d8729f02fe6d1
}

build() {
  make install-bin DESTDIR=/dst PREFIX=
}
