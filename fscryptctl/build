check() {
  url https://github.com/google/fscryptctl/tags
  match version 1.3.0 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack fscryptctl-1.3.0.tar.gz \
    https://github.com/google/fscryptctl/archive/refs/tags/v1.3.0.tar.gz
  apply cosmetic.diff
  tree 58d3be52c58856516e6829f3421a0ebffb3bbc158f8652918272b13946df2df0
}

build() {
  make install-bin DESTDIR=/dst PREFIX=
}
