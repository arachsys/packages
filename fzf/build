require go

check() {
  url https://github.com/junegunn/fzf/tags
  match version 0.67.0 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack fzf-0.67.0.tar.gz \
    https://github.com/junegunn/fzf/archive/refs/tags/v0.67.0.tar.gz
  go mod vendor
  tree ca53b07af12f66aab0819e6652881e972d1448254f246fc9b804f3c32666b2a3
}

build() {
  go build -ldflags "-s -w -X main.version=0.67.0 -X main.revision=arachsys"
  install -D fzf /dst/bin/fzf
}
