require go

check() {
  url https://github.com/junegunn/fzf/tags
  match version 0.66.1 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack fzf-0.66.1.tar.gz \
    https://github.com/junegunn/fzf/archive/refs/tags/v0.66.1.tar.gz
  go mod vendor
  tree c6205b18a4cee378611396f3e1acc6760f4e5cfdaa71a5fce4b9e10ffe7f33c3
}

build() {
  go build -ldflags "-s -w -X main.version=0.66.1 -X main.revision=arachsys"
  install -D fzf /dst/bin/fzf
}
