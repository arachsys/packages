require go

check() {
  url https://github.com/junegunn/fzf/tags
  match version 0.66.0 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack fzf-0.66.0.tar.gz \
    https://github.com/junegunn/fzf/archive/refs/tags/v0.66.0.tar.gz
  go mod vendor
  tree e85ee80791c1ad6ffab907ff17cb526dfa5e3cfcbd0230a7021f524b9f804fdb
}

build() {
  go build -ldflags "-s -w -X main.version=0.66.0 -X main.revision=arachsys"
  install -D fzf /dst/bin/fzf
}
