require go

check() {
  url https://github.com/junegunn/fzf/tags
  match version 0.44.0 'tags/([0-9.]+)\.tar'
}

prepare() {
  unpack fzf-0.44.0.tar.gz \
    https://github.com/junegunn/fzf/archive/refs/tags/0.44.0.tar.gz
  go mod vendor
  tree 06a4e5e0915f14667ea7e9dc9a02472a3387e0fdd80851c40da20def0b07ac0d
}

build() {
  go build -ldflags "-s -w -X main.version=0.44.0 -X main.revision=arachsys"
  install -D fzf /dst/bin/fzf
}
