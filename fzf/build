require go

check() {
  url https://github.com/junegunn/fzf/tags
  match version 0.38.0 'tags/([0-9.]+)\.tar'
}

prepare() {
  unpack fzf-0.38.0.tar.gz \
    https://github.com/junegunn/fzf/archive/refs/tags/0.38.0.tar.gz
  go mod vendor
  tree d00eb4374a3d38e173a54ac9ed287a134b438445a4c5a38edaae7c2d06b8074b
}

build() {
  go build -ldflags "-s -w -X main.version=0.38.0 -X main.revision=arachsys"
  install -D fzf /dst/bin/fzf
}
