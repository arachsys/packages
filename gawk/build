mode autotools

check() {
  url https://ftp.gnu.org/gnu/gawk/
  match version 5.2.0 'gawk-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.gnu.org/gnu/gawk/gawk-5.2.0.tar.xz
  tree 89cd25c81b9d02e3513ec313568e01c88509d8de93e585fc2452d7a0ffca1329
}

post-install() {
  rm -f /dst/bin/{pgawk,gawk}-*
}
