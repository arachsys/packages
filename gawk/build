mode autotools

check() {
  url https://ftp.gnu.org/gnu/gawk/
  match version 5.2.1 'gawk-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.gnu.org/gnu/gawk/gawk-5.2.1.tar.xz
  tree d6ad5efdd41d5276ce0e9217606f93036281a166c699ad6ae0a6db10dd73e52a
}

post-install() {
  rm -f /dst/bin/gawk{-*,bug}
}
