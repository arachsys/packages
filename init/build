check() {
  url https://github.com/arachsys/init/tags
  match version 2.16 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.16.tar.gz
  apply sync.diff
  tree 45162c5296dd7c13590621a505c32073276e7b70cd3d685562b3902b03ba2e1d
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
