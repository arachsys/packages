check() {
  url https://github.com/arachsys/init/tags
  match version 2.6 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.6.tar.gz
  tree 47a5deea598a87bd6a2c3e5c0d9e7dc9ac1d36ed2c18dc8daa9625be713647db
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
