check() {
  url https://github.com/arachsys/init/tags
  match version 2.3 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.3.tar.gz
  tree 7ab286c57aac55281a77d23d3831e07ee202eef44efe9f0c7075183d4da05c61
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
