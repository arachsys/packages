check() {
  url https://github.com/arachsys/init/tags
  match version 2.19 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.19.tar.gz
  apply sync.diff
  tree 0c1ed9a2f70f4df37838946e186ff3d224e21786ced0e643aa3808be0c0336e9
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
