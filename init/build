check() {
  url https://github.com/arachsys/init/tags
  match version 2.9 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.9.tar.gz
  apply sync.diff
  tree 35a81450537f4365f9ac91fe80c1710472dadbc6c33474c0d367591df30e00ee
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
