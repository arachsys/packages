check() {
  url https://github.com/arachsys/init/tags
  match version 2.11 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.11.tar.gz
  apply sync.diff
  tree a338eb10966c065d8b5d975756e9251b667398556d7016ca73837c7a5928699e
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
