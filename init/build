check() {
  url https://github.com/arachsys/init/tags
  match version 2.18 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.18.tar.gz
  apply sync.diff
  tree cdb83a163798dc437e563271519bcc95b28cd6323e09cd6147f07c16082c47e5
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
