check() {
  url https://github.com/arachsys/init/tags
  match version 2.14 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.14.tar.gz
  apply sync.diff
  tree b4960d52952b5b6158e4c83bec69d7ccf7b036e9bf40adbd415286b905c46af6
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
