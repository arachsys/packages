check() {
  url https://github.com/arachsys/init/tags
  match version 2.1 'init-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/init/archive/init-2.1.tar.gz
  tree 50bd8c3b5a5da5d8a13f800536b1a534c0589f1f916c99add023e040c5f4ce56
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
  cp /src/examples/bin/* /dst/bin/
}
