mode autotools

check() {
  url http://www.greenwoodsoftware.com/less/download.html
  match version 598 'less-([0-9.]+)\.tar'
}

prepare() {
  unpack http://www.greenwoodsoftware.com/less/less-598.tar.gz
  tree 60517f2f55c5803fcaec24089f264ea9afadf166dac4ef2fef9f6d66595102d2
}

post-install() {
  ln -s less /dst/bin/more
}
