check() {
  url https://github.com/arachsys/libelf/tags
  match version 0.187 'tags/v([0-9.]+)\.tar'

  url https://sourceware.org/git/elfutils.git/tags
  match 'upstream elfutils' 0.187 'elfutils-([0-9.]+)'
}

prepare() {
  unpack libelf-0.187.tar.gz \
    https://github.com/arachsys/libelf/archive/refs/tags/v0.187.tar.gz
  tree b5674112973e8750526565382aba2227e73d29639bb8ffb8d529297395db7361
}

build() {
  make install DESTDIR=/dst INCDIR=/lib/include
}
