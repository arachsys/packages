check() {
  url https://github.com/arachsys/libelf/releases
  match version 0.186 'v([0-9.]+)\.tar'

  url https://sourceware.org/git/elfutils.git/tags
  match 'upstream elfutils' 0.186 'elfutils-([0-9.]+)'
}

prepare() {
  unpack libelf-0.186.tar.gz \
    https://github.com/arachsys/libelf/archive/refs/tags/v0.186.tar.gz
  tree f1c903da2dfcb3683a5477f4fda7504e6da73afb0a3578a2bcd9103e8b237106
}

build() {
  make install DESTDIR=/dst INCDIR=/lib/include
}
