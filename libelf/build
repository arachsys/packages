check() {
  url https://github.com/arachsys/libelf/tags
  match version 0.192 'tags/v([0-9.]+)\.tar'

  url https://sourceware.org/git/elfutils.git/tags
  match 'upstream elfutils' 0.192 'elfutils-([0-9.]+)'
}

prepare() {
  unpack libelf-0.192.tar.gz \
    https://github.com/arachsys/libelf/archive/refs/tags/v0.192.tar.gz
  tree 1eeaa89d0ee0341c104afd9879b4f23bee33a214c06027105eacf194052e5c96
}

build() {
  make install DESTDIR=/dst INCDIR=/lib/include
}
