check() {
  url https://github.com/arachsys/libelf/tags
  match version 0.194 'tags/v([0-9.]+)\.tar'

  url https://sourceware.org/git/elfutils.git/tags
  match 'upstream elfutils' 0.194 'elfutils-([0-9.]+)'
}

prepare() {
  unpack libelf-0.194.tar.gz \
    https://github.com/arachsys/libelf/archive/refs/tags/v0.194.tar.gz
  tree aba705a57885e00f35c25002b1d1af2f4f1013010840e9e8ce9f7f9118b54b9c
}

build() {
  make install DESTDIR=/dst INCDIR=/lib/include
}
