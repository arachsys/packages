check() {
  url https://github.com/arachsys/libelf/tags
  match version 0.190 'tags/v([0-9.]+)\.tar'

  url https://sourceware.org/git/elfutils.git/tags
  match 'upstream elfutils' 0.190 'elfutils-([0-9.]+)'
}

prepare() {
  unpack libelf-0.190.tar.gz \
    https://github.com/arachsys/libelf/archive/refs/tags/v0.190.tar.gz
  tree 29ec403cca55ec0bc272efd6bdf91b9c70a84d712ae73ea6cb42ad019ca78f2d
}

build() {
  make install DESTDIR=/dst INCDIR=/lib/include
}
