mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.5.2 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.5.2.tar.gz
  tree dee27f5e45b9947db5aede108446ba1dbfe9a3b9d37b80d41cc84956247cdece
}

config-extra --enable-nc
