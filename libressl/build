mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.7.0 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.7.0.tar.gz
  tree 6c1d48669a0a62e02978d8bf7e1ec2ff4f36313a8a781dd650c4b74052566b97
}

config-extra --enable-nc
