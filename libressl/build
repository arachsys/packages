mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.5.3 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.5.3.tar.gz
  tree 76f9f2a7d79fab9a18dbddfe1f056b6ff6f3f4ef23b9b365202591f719825483
}

config-extra --enable-nc
