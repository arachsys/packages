mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.7.1 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.7.1.tar.gz
  tree 9125f0f9cd606d530262c9c6d1f46be0be0c0e4efcb0843a82bf0b38d9bd7386
}

config-extra --enable-nc
