mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.7.2 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.7.2.tar.gz
  tree 616b62b6eecb3964d3f49be43e2bfb335772f770287f970b57c1563d9d8b97d4
}

config-extra --enable-nc
