mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.3.4 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.3.4.tar.gz
  tree f98f059f680f898695d428a79623cb1d584fa058d4193c3378c452e695c1541e
}

config-extra --enable-nc
