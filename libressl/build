mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.6.1 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.6.1.tar.gz
  tree a6f2b490684a7808ea33288129db3de46b08814a0e3a33a735a3f35b2b931542
}

config-extra --enable-nc
