mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.4.0 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.4.0.tar.gz
  tree 37825ca875612cf5ff8cc2173ef12079a42f42e6514f69d29834d23da10edb26
}

config-extra --enable-nc
