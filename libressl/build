mode autotools

check() {
  url https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/
  match version 3.6.0 'libressl-([0-9.]+)\.tar'
}

prepare() {
  unpack https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-3.6.0.tar.gz
  tree 3a325695a539f2e3fafd9978dc2d3282a1f4d6c259fc4505fc7643b9acd4201b
}

config-extra --enable-nc
