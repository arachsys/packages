mode autotools

check() {
  url https://download.libsodium.org/libsodium/releases/
  match version 1.0.18 'libsodium-([0-9.]+)\.tar'
}

prepare() {
  unpack https://download.libsodium.org/libsodium/releases/libsodium-1.0.18.tar.gz
  tree 7354ff373a09c3dafb6e2ba1a15a74a00b02958aba38f35bf49ae6370796f4af
}
