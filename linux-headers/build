prepare() {
  unpack https://www.kernel.org/pub/linux/kernel/v5.x/linux-5.12.tar.xz
  tree caaafa22138512e65d2cb812197c834adae786280c53364a095c1f72eabdf9f1
}

build() {
  make headers HOSTCC=cc
  find usr/include -type f -name '*.h' -exec cp -t /dst --parents {} +
  find usr/include -type l -exec cp -d -t /dst --parents {} +
  mv /dst/usr /dst/lib
}
