check() {
  url https://www.kernel.org/
  match linux 5.13.11 'linux-([0-9.]+)\.tar'
}

prepare() {
  unpack https://www.kernel.org/pub/linux/kernel/v5.x/linux-5.13.11.tar.xz
  tree c6c4fb6a3cdfa5d7341b24f014b5c6574d55bf47809a6d8c23a416226e2fa74b
}

build() {
  make headers CC=cc HOSTCC=cc
  find usr/include -type f -name '*.h' -exec cp -t /dst --parents {} +
  find usr/include -type l -exec cp -d -t /dst --parents {} +
  mv /dst/usr /dst/lib
}
