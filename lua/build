check() {
  url https://www.lua.org/ftp/
  match version 5.5.0 'lua-([0-9.]+)\.tar'
}

prepare() {
  unpack https://www.lua.org/ftp/lua-5.5.0.tar.gz
  apply paths.diff
  tree e7d5522c0e589f82fcaedf6ce1ac0f3b2001a9188723738df8144705bc8256c3
}

build() {
  make linux CC=cc
  make install INSTALL_TOP=/dst
}
