mode autotools

check() {
  url https://www.nano-editor.org/dist/latest/
  match version 5.9 'nano-([0-9.]+)\.tar'
}

prepare() {
  unpack https://www.nano-editor.org/dist/v5/nano-5.9.tar.xz
  tree e9bee5046578507ff36bd588e351b98d3eb03f1a9cb2d5c2894766a11cc11f3b
}

post-install() {
  rm -f /dst/bin/rnano
}
