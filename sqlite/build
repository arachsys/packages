check() {
  url https://www.sqlite.org/index.html
  match version 3.44.2 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.44.2.tar.gz \
    https://www.sqlite.org/2023/sqlite-autoconf-3440200.tar.gz
  tree 53b5b7e7727b377ea84e67577411dcf88bb8151c85a35deaee5fbcbfdc75799a
}

build() {
  auto-config --disable-static-shell
  make install DESTDIR=/dst
}
