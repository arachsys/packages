mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.38.2 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.38.2.tar.gz \
    https://www.sqlite.org/2022/sqlite-autoconf-3380200.tar.gz
  tree 40066e5ca58a1a70ee17d9ea61918424fd8e0c5c753ae4ca1d31d8853d24f3ab
}

config-extra --disable-static-shell
