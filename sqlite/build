mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.42.0 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.42.0.tar.gz \
    https://www.sqlite.org/2023/sqlite-autoconf-3420000.tar.gz
  tree dd9c97423598f4bed7d4aff0869adb86b5fb1ee45ecdbe40c1c1da8a75394e63
}

config-extra --disable-static-shell
