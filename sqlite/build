mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.41.1 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.41.1.tar.gz \
    https://www.sqlite.org/2023/sqlite-autoconf-3410100.tar.gz
  tree 04a4f12317efc3c5a43e38e1b8381a932e78ceacdb06b02f973212034d6709e7
}

config-extra --disable-static-shell
