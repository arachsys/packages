mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.39.0 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.39.0.tar.gz \
    https://www.sqlite.org/2022/sqlite-autoconf-3390000.tar.gz
  tree 74e196ed2e3a11aa1e229da7497bfe3aede0d689b77052f51f1b01e7458b1bd8
}

config-extra --disable-static-shell
