mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.40.1 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.40.1.tar.gz \
    https://www.sqlite.org/2022/sqlite-autoconf-3400100.tar.gz
  tree ae0394cd36170f0a68d613a51e306aa591169b6985777c710e7924fcccbb185e
}

config-extra --disable-static-shell
