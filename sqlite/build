mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.37.1 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.37.1.tar.gz \
    https://www.sqlite.org/2021/sqlite-autoconf-3370100.tar.gz
  tree 2e70355e1a10414b368b0dfdc32fc635cdf94ea2103b039fb34b71672cd345a3
}

config-extra --disable-static-shell
