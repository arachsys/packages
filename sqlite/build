check() {
  url https://www.sqlite.org/index.html
  match version 3.43.0 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.43.0.tar.gz \
    https://www.sqlite.org/2023/sqlite-autoconf-3430000.tar.gz
  tree b769c90dd3b6f6b407fa56b2c413520ce1c29653a85edc3c240aac630f6da011
}

build() {
  auto-config --disable-static-shell
  make install DESTDIR=/dst
}
