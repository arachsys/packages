mode autotools

check() {
  url https://www.sqlite.org/index.html
  match version 3.41.0 'Version ([0-9.]*[0-9])'
}

prepare() {
  unpack sqlite-3.41.0.tar.gz \
    https://www.sqlite.org/2023/sqlite-autoconf-3410000.tar.gz
  tree a0810412273d6a57a2bee713548125e63c0d68c64cc4b67ef8d1191669734e3f
}

config-extra --disable-static-shell
