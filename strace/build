check() {
  url https://github.com/strace/strace/tags
  match version 6.19 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack https://strace.io/files/6.19/strace-6.19.tar.xz
  tree e9003f26e97aa32d9dcca3060001ca8ce2d732be3a5984626cb2030b186e62cc
}

build() {
  auto-config --disable-gcc-Werror --disable-mpers
  make install DESTDIR=/dst
  rm -f /dst/bin/strace-{graph,log-merge}
}
