mode autotools

check() {
  url https://github.com/strace/strace/releases
  match version 5.14 'strace-([0-9.]+)\.tar'
}

prepare() {
  unpack https://strace.io/files/5.14/strace-5.14.tar.xz
  tree ca62bd9200454028ff01e0205f2c7019dca051154555625dfef95a885473d35c
}

config-extra --disable-gcc-Werror --disable-mpers

post-install() {
  rm -f /dst/bin/strace-{graph,log-merge}
}
