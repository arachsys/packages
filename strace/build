check() {
  url https://github.com/strace/strace/tags
  match version 6.5 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack https://strace.io/files/6.5/strace-6.5.tar.xz
  tree 25fa3d56a1dad42d6ad19c46e90a70ccd0c672c3b69077a351d74a03249915f0
}

build() {
  auto-config --disable-gcc-Werror --disable-mpers --enable-bundled
  make install DESTDIR=/dst
  rm -f /dst/bin/strace-{graph,log-merge}
}
