check() {
  url https://github.com/strace/strace/tags
  match version 6.6 'tags/v([0-9.]+)\.tar'
}

prepare() {
  unpack https://strace.io/files/6.6/strace-6.6.tar.xz
  tree 4f647bd1baa64741db1bccd15983f0b22b09184b4d00efd57aa6f1a956517fda
}

build() {
  auto-config --disable-gcc-Werror --disable-mpers --enable-bundled
  make install DESTDIR=/dst
  rm -f /dst/bin/strace-{graph,log-merge}
}
