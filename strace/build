mode autotools

check() {
  url https://github.com/strace/strace/releases
  match version 5.16 'strace-([0-9.]+)\.tar'
}

prepare() {
  unpack https://strace.io/files/5.16/strace-5.16.tar.xz
  tree 404950ad24b5afbe3404488e6b1f9b2d6f9b7aa9b47ddc4f0b6af280b9c737fd
}

config-extra --disable-gcc-Werror --disable-mpers

post-install() {
  rm -f /dst/bin/strace-{graph,log-merge}
}
