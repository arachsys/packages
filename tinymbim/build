check() {
  url https://api.github.com/repos/arachsys/tinymbim/branches/master
  match snapshot 20220731 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack tinymbim-20220731-9968c30a.tar.gz \
    https://github.com/arachsys/tinymbim/archive/9968c30af25dc66e74858341219a1e4e8b0df056.tar.gz
  tree f717927d1b023f7f7b3968e2b2e61eeb1073555066c9db4c725afe2e8a58a0aa
}

build() {
  make install DESTDIR=/dst
}
