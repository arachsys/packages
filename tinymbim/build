check() {
  url https://api.github.com/repos/arachsys/tinymbim/branches/master
  match snapshot 20220910 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack tinymbim-20220910-82c3ed8a.tar.gz \
    https://github.com/arachsys/tinymbim/archive/82c3ed8a.tar.gz
  tree 3092a902fd5e385ffbac8ad4b96dd35874961c323453ccc398c525d8004f3b9e
}

build() {
  make install DESTDIR=/dst
}
