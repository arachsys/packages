check() {
  url https://github.com/arachsys/tinymbim/tags
  match version 1.0 'tags/tinymbim-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/tinymbim/archive/refs/tags/tinymbim-1.0.tar.gz
  tree b9b5f727918013a0a7e75e55460879c559576a879ffa727a06ba61a3a6aa0f3c
}

build() {
  make install DESTDIR=/dst
}
