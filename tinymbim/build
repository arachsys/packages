check() {
  url https://github.com/arachsys/tinymbim/tags
  match version 1.2 'tags/tinymbim-([0-9.]+)\.tar'
}

prepare() {
  unpack https://github.com/arachsys/tinymbim/archive/refs/tags/tinymbim-1.2.tar.gz
  tree d903870f65251414e49656b5675c98f85b2dfd2a3c9eb75e636b16ecafb9191f
}

build() {
  make install DESTDIR=/dst
}
