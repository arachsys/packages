check() {
  url https://api.github.com/repos/arachsys/tinyquota/branches/master
  match snapshot 20230416 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack tinyquota-20230416-556c63cc.tar.gz \
    https://github.com/arachsys/tinyquota/archive/556c63cc.tar.gz
  tree e12557030059fc5914698881ce888baf603f6a0faec11a05db7dcf62e966eafa
}

build() {
  make install DESTDIR=/dst
}
