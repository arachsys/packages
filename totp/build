check() {
  url https://api.github.com/repos/arachsys/totp
  match snapshot 20210313 \
    '"pushed_at": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack totp-20210313-7cf00a65.tar.gz \
    https://github.com/arachsys/totp/archive/7cf00a65fcbdccb78517f96cee07bf19329c03bd.tar.gz
  tree c67467eb7262840a752b29ae6a9e1d2eadb4b580c42ec58f7ad0683f0f029b82
}

build() {
  make install CC=cc DESTDIR=/dst PREFIX=
}
