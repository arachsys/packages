enable setuid

check() {
  url https://api.github.com/repos/arachsys/arachsys-utils/branches/master
  match snapshot 20251205 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack arachsys-utils-20251205-0d981160.tar.gz \
    https://github.com/arachsys/arachsys-utils/archive/0d981160.tar.gz
  tree 4ff552545aa3620746ffe5c4e697e5715a60f03444986d6306b805ed5d642611
}

build() {
  make install DESTDIR=/dst
}
