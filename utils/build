enable setuid

check() {
  url https://api.github.com/repos/arachsys/arachsys-utils/branches/master
  match snapshot 20260218 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack arachsys-utils-20260218-1392dbbb.tar.gz \
    https://github.com/arachsys/arachsys-utils/archive/1392dbbb.tar.gz
  tree f598855cc82afc81d9222e313a163da79a6fc2b2d2adb54771eddc9a58b9f5eb
}

build() {
  make -C host install DESTDIR=/img/host-utils
  make -C user install DESTDIR=/img/user-utils
}
