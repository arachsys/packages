enable setuid

check() {
  url https://api.github.com/repos/arachsys/arachsys-utils/branches/master
  match snapshot 20260111 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack arachsys-utils-20260111-51d376f0.tar.gz \
    https://github.com/arachsys/arachsys-utils/archive/51d376f0.tar.gz
  tree 6f14c58f6dffd1ea6ec4a432a86cdc1b66acc961c214c52d8a866786cabe076f
}

build() {
  make -C host install DESTDIR=/img/host-utils
  make -C user install DESTDIR=/img/user-utils
}
