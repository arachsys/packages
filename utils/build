enable setuid

check() {
  url https://api.github.com/repos/arachsys/arachsys-utils/branches/master
  match snapshot 20251202 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack arachsys-utils-20251202-ef4b12c2.tar.gz \
    https://github.com/arachsys/arachsys-utils/archive/ef4b12c2.tar.gz
  tree 78c61ff96c2d12f76bf9ca4b0d05e739cea3334277362b05ee8a90795f225a50
}

build() {
  make install DESTDIR=/dst
}
