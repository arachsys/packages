enable setuid

check() {
  url https://api.github.com/repos/arachsys/arachsys-utils/branches/master
  match snapshot 20251128 \
    '"date": *"([0-9]{4})-([0-9]{2})-([0-9]{2})' '\1\2\3'
}

prepare() {
  unpack arachsys-utils-20251128-4e5d68e0.tar.gz \
    https://github.com/arachsys/arachsys-utils/archive/4e5d68e0.tar.gz
  tree 200382defe8b25d45c5af5f219e670581fe294b5850b89fbfaeda8fa66191558
}

build() {
  make install DESTDIR=/dst
}
