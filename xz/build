check() {
  url https://tukaani.org/xz/
  match version 5.4.3 'xz-([0-9.]+)\.tar'
}

prepare() {
  unpack https://tukaani.org/xz/xz-5.4.3.tar.xz
  tree a6a8dfc5fb23bda35fd80c8deff4b35c2a148a68ac5a76125506b3087db786bc
}

build() {
  auto-config
  make install DESTDIR=/dst
  rm -f /dst/bin/{lz,xz}{cmp,diff,*grep,less,more} \
    /dst/lib/man/man1/{lz,xz}{cmp,diff,*grep,less,more}.1
}
